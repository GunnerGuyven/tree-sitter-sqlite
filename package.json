{
    "name": "tree-sitter-sqlite",
    "version": "0.0.1",
    "description": "SQLite grammar for the tree-sitter parsing library",
    "main": "bindings/node",
    "author": "h11 <nirisix@gmail.com>",
    "license": "MIT",
    "tree-sitter": [
        {
            "scope": "source.sql",
            "file-types": [
                "sql"
            ]
        }
    ],
    "dependencies": {
        "nan": "2.14.2"
    },
    "devDependencies": {
        "prettier": "2.3.0",
        "tree-sitter-cli": "0.19.5"
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": false,
        "semi": false,
        "singleQuote": false,
        "quoteProps": "as-needed",
        "trailingComma": "all",
        "bracketSpacing": true,
        "arrowParens": "always",
        "endOfLine": "lf"
    }
}
