{
    "name": "tree-sitter-sql",
    "version": "0.0.1",
    "description": "SQL grammar for the tree-sitter parsing library",
    "author": "H11",
    "license": "MIT",
    "dependencies": {
        "nan": "2.14.2"
    },
    "devDependencies": {
        "prettier": "2.3.0",
        "tree-sitter-cli": "0.19.5"
    },
    "tree-sitter": [
        {
            "scope": "source.sql.sqlite",
            "file-types": ["sql"],
            "path": "sqlite",
            "highlights": "sqlite/queries/highlights.scm",
            "injection-regex": "^(sql)$"
        },
        {
            "scope": "source.sql.postgresql",
            "file-types": ["sql"],
            "path": "postgresql",
            "highlights": "postgresql/queries/highlights.scm",
            "injection-regex": "^(sql)$"
        }
    ]
}
